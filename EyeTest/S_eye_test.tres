}

void light() {
// Input:4
	vec3 n_out4p0 = NORMAL;


// Input:2
	vec3 n_out2p0 = LIGHT;


// DotProduct:3
	float n_out3p0 = dot(n_out4p0, n_out2p0);


// Clamp:5
	float n_in5p1 = -0.90000;
	float n_in5p2 = 0.90000;
	float n_out5p0 = clamp(n_out3p0, n_in5p1, n_in5p2);


// FloatOp:8
	float n_in8p1 = 1.00000;
	float n_out8p0 = n_out5p0 + n_in8p1;


// Input:10
	float n_out10p0 = ATTENUATION;


// FloatOp:11
	float n_in11p1 = 2.10000;
	float n_out11p0 = n_out10p0 / n_in11p1;


// FloatOp:9
	float n_out9p0 = n_out8p0 + n_out11p0;


// Clamp:12
	float n_in12p1 = 0.05000;
	float n_in12p2 = 0.95000;
	float n_out12p0 = clamp(n_out9p0, n_in12p1, n_in12p2);


// VectorCompose:13
	float n_in13p1 = 0.00000;
	vec2 n_out13p0 = vec2(n_out12p0, n_in13p1);


// Texture2D:6
	vec4 n_out6p0 = texture(tex_lgt_6, n_out13p0);


// Input:17
	vec3 n_out17p0 = LIGHT_COLOR;


// VectorOp:16
	vec4 n_out16p0 = n_out6p0 * vec4(n_out17p0, 0.0);


// Input:15
	vec3 n_out15p0 = DIFFUSE_LIGHT;


// VectorOp:18
	vec4 n_out18p0 = n_out16p0 + vec4(n_out15p0, 0.0);


// Clamp:19
	vec4 n_in19p1 = vec4(0.00000, 0.00000, 0.00000, 0.00000);
	vec4 n_in19p2 = vec4(1.00000, 1.00000, 1.00000, 1.00000);
	vec4 n_out19p0 = clamp(n_out18p0, n_in19p1, n_in19p2);


// Output:0
	DIFFUSE_LIGHT = vec3(n_out19p0.xyz);


}
