script/source = "@tool
extends Node3D


var face_material: Material
@onready var face_placeholder: MeshInstance3D = $Example1/FacePlaceholder
@onready var mouth_controller: Node3D = $Example1/FacePlaceholder/MouthController



func _ready() -> void:
	face_material = face_placeholder.mesh.surface_get_material(0)


func _process(_delta: float) -> void:
	var texture_position: Vector2 = Vector2(
		-mouth_controller.position.x,
		-mouth_controller.position.z)
	texture_position = texture_position / 2.0
	face_material.set(\"shader_parameter/position\", texture_position)
"

