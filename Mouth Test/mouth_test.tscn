code = "shader_type spatial;
render_mode blend_mix, depth_draw_opaque, cull_back, diffuse_lambert, specular_schlick_ggx;

uniform sampler2D skin_texture : source_color;
uniform vec2 position;
uniform sampler2D mouth : source_color, repeat_disable;



void fragment() {
	vec4 n_out2p0;
// Texture2D:2
	n_out2p0 = texture(skin_texture, UV);


// Input:10
	vec2 n_out10p0 = UV;


// Vector2Parameter:8
	vec2 n_out8p0 = position;


// VectorOp:9
	vec2 n_out9p0 = n_out10p0 + n_out8p0;


	vec4 n_out4p0;
// Texture2D:4
	n_out4p0 = texture(mouth, n_out9p0);


// FloatOp:7
	float n_in7p0 = 1.00000;
	float n_out7p0 = n_in7p0 - n_out4p0.x;


// Mix:5
	vec4 n_out5p0 = mix(n_out2p0, n_out4p0, n_out7p0);


// Output:0
	ALBEDO = vec3(n_out5p0.xyz);


}
